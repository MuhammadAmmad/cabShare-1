<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape >
            <solid
                android:color="#aaf39c00" />
            <stroke
                android:width="1dp"
                android:color="#FFBB00" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="5dp"
                android:right="5dp"
                android:top="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    <item >
        <shape >
            <gradient
                android:startColor="#FFBB00"
                android:endColor="#FFBB00"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ae7205" />
            <corners
                android:radius="4dp" />
            <padding
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>